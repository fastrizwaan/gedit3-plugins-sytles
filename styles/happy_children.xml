  <style name="def:statement"	foreground="#991d54" />
  <style name="def:string"	foreground="#0e7e00" />
  <style name="def:type"	foreground="#991d54" />
  <style name="draw-spaces"	foreground="#404040" />
  <style name="line-numbers"	foreground="#808080" background="#101010" />
  <style name="search-match"	foreground="#000000" background="#1a5fb4" />
