<style-scheme name="Happy Children" id="happy_children" version="1.0">
  <author>Mohammed Asif Ali Rizvan</author>
  <description>Based on Dull living color scheme for Gedit3.</description>

  <style name="bracket-match"	foreground="#f8eca8" bold="true" />
  <style name="current-line"	background="#202329" />
  <style name="cursor"	foreground="#5cb0f5" />
  <style name="def:base-n-integer"	foreground="#b57106" />
  <style name="def:boolean"	foreground="#b57106" />
  <style name="def:builtin"	foreground="#991d54" />
  <style name="def:character"	foreground="#0e7e00" />
  <style name="def:comment"	foreground="#808080" />
  <style name="def:complex"	foreground="#b57106" />
  <style name="def:constant"	foreground="#b57106" />
  <style name="def:decimal"	foreground="#d19a66" />
  <style name="def:doc-comment-element"	foreground="#5f5a60" />
  <style name="def:doc-comment"	foreground="#bc9458" />
  <style name="def:error"	foreground="#efefef" background="#282c34" />
  <style name="def:floating-point"	foreground="#b57106" />
  <style name="def:function"	foreground="#e6d6b4" />
  <style name="def:identifier"	foreground="#991d54" />
