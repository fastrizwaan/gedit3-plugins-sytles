  <style name="def:note"	foreground="#5f5a60" />
  <style name="def:number"	foreground="#b57106" />
  <style name="def:operator"	foreground="#e6d6b4" />
  <style name="def:preprocessor"	foreground="#991d54" />
  <style name="def:reserved"	foreground="#cc7833" />
  <style name="def:shebang"	foreground="#b57106" />
